{
    "name": "assayer-pro/yii2-sitemap-module",
    "description": "Yii2 module for automatically generation XML Sitemap",
    "keywords": ["yii2", "xml", "sitemap", "module"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "source": "https://github.com/assayer-pro/yii2-sitemap-module",
        "issues": "https://github.com/assayer-pro/yii2-sitemap-module/issues"
    },
    "authors": [
        {
            "name": "Assayer Pro",
            "homepage" : "https://github.com/assayer-pro"
        }
    ],
    "require": {
        "yiisoft/yii2": ">=2.0.6"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "~2.0.0",
        "yiisoft/yii2-faker": "*",
        "codeception/codeception": "2.0.*"
    },
    "autoload": {
        "psr-4": {
            "assayerpro\\sitemap\\": ""
        }
    }
}
